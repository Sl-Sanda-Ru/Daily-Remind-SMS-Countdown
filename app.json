{
    "name": "Daily-Remind-SMS-Countdown",
    "description": "With This Program You Can Set A Daily Reminder With SMS",
    "logo": "https://pics.freeicons.io/uploads/icons/png/6029094171580282643-512.png",
    "keywords": ["Reminder", "SMS", "countdown"],
    "repository": "https://github.com/Sl-Sanda-Ru/Daily-Remind-SMS-Countdown",
    "website":   "https://t.me/Sl_Sanda_Ru",
    "success_url": "https://t.me/Sl_Sanda_Ru",
    "env": {
        "NUM": {
            "description": "The Recipient Phone Number To Send SMS Reminder. Format +947xxxxxxxx",
            "required": true,
            "value": "94771234567"
        },
        "DAY": {
            "description": "The Day For CountDown. Format XXXX-XX-XX",
